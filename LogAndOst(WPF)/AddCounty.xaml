<Window x:Class="LogAndOst_WPF_.AddCounty"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LogAndOst_WPF_"
        mc:Ignorable="d"
        Title="AddCounty" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Label Content="Введите название страны" Margin="10" HorizontalAlignment="Center"></Label>
            <TextBox x:Name="CountryBox" Height="30" Width="300"></TextBox>
            <Button x:Name="AddButton" Content="Добавить страну" Height="30" Margin="10" Click="AddButton_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
